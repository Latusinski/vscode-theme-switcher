{
    "name": "theme-switcher",
    "displayName": "theme-switcher",
    "description": "",
    "version": "0.1.0",
    "publisher": "latusinski",
    "repository": "https://github.com/Latusinski/vscode-theme-switcher",
    "engines": {
        "vscode": "^1.16.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:theme_swapper.swapTheme"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "theme_swapper.swapTheme",
                "title": "Swap Theme"
            }
        ],
        "keybindings": [
            {
                "command": "theme_swapper.swapTheme",
                "key": "ctrl+alt+T",
                "mac": "cmd+alt+T"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.48",
        "@types/node": "^7.0.61",
        "mocha": "^3.5.3",
        "typescript": "^2.8.3",
        "vscode": "^1.1.16"
    }
}
