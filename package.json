{
  "name": "theme-switcher",
  "displayName": "theme-switcher",
  "description": "",
  "version": "1.2.1",
  "publisher": "latusinski",
  "repository": "https://github.com/Latusinski/vscode-theme-switcher",
  "engines": {
    "vscode": "^1.23.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension",
  "contributes": {
    "configuration": [
      {
        "title": "Theme Switcher",
        "properties": {
          "theme_switcher.light_theme": {
            "type": "string",
            "default": "Default Light+",
            "description": "The light theme to use, when switching themes.",
            "scope": "application"
          },
          "theme_switcher.dark_theme": {
            "type": "string",
            "default": "Default Dark+",
            "description": "The dark theme to use, when switching themes.",
            "scope": "application"
          }
        }
      }
    ],
    "commands": [
      {
        "command": "theme_switcher.switchTheme",
        "title": "Swap Theme"
      }
    ],
    "keybindings": [
      {
        "command": "theme_switcher.switchTheme",
        "key": "ctrl+alt+T",
        "mac": "cmd+alt+T"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "compile": "webpack --mode none",
    "watch": "webpack --mode none --watch",
    "test-compile": "tsc -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run test-compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.1",
    "@types/suncalc": "^1.8.0",
    "mocha": "^7.1.0",
    "ts-loader": "^8.0.1",
    "typescript": "^3.8.3",
    "vscode": "^1.1.36",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "dependencies": {
    "got": "^11.0.1",
    "suncalc": "^1.8.0"
  }
}
